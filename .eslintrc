{
  "plugins": [ "jasmine" ],
  "parserOptions": {
    "ecmaVersion": 2017,
    "sourceType": "module"
  },
  "rules": {
    "jasmine/no-focused-tests": "error"
  },
  "env": {
    "es6": true,
    "node": true,
    "jasmine": true
  },
  "globals": {
    "XMLHttpRequest": true,
    "FileReader": true
  },
  "extends": "standard"
}
